{
  "rules": [
    {
      "enabled": true,
      "name": "mtolv/teste-Delete-LastAccess-2",
      "type": "Lifecycle",
      "definition": {
        "actions": {
          "baseBlob": {
            "delete": {
              "daysAfterLastAccessTimeGreaterThan": 2
            }
          }
        },
        "filters": {
          "blobTypes": [
            "blockBlob"
          ],
          "prefixMatch": [
            "mtolv/teste"
          ]
        }
      }
    }
  ]
}